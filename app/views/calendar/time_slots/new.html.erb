<div id="time_slot_form">
  <%= simple_form_for (@time_slot || TimeSlot.new),
  url: calendar_time_slots_path do |f| %>
    <%= f.hidden_field :doctor_id, value: @doctor_id %>
    <%= f.input :start_date, label: 'Date' %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
<%= link_to "Cancel", calendar_path %>
