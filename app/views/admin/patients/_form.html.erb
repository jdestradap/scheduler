<div id="patient_form">
  <%= simple_form_for (@patient || Patient.new),
  url: url do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.fields_for :user do |builder| %>
      <%= builder.input :email %>
      <%= builder.input :password %>
    <% end %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
